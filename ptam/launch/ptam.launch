<launch>
    <node name="ptam" pkg="ptam" type="ptam" clear_params="true" output="screen">
	    <remap from="image" to="/ptam/camera/image_raw" />
        <remap from="pose" to="pose"/>
	    <rosparam file="$(find ptam)/PtamFixParams.yaml"/>
    </node>
<!--     <include file="$(find bird_vision)/launch/chameleon.launch" /> -->
<node name="republish" pkg="image_transport" type="republish" args="compressed in:=/chameleon/camera/image_raw raw out:=/ptam/camera/image_raw" output="screen" />    
</launch>

